<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="sapui5_xml.Categories" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Categories" >
		<content>
			<Button text="products" press="gotoProducts"></Button>
			<List id="listC" items="{data1>/Categories}">
				<items>
					<DisplayListItem label="{data1>CategoryName}" value="{data1>CategoryID}"/>
				</items>				
			</List>
		</content>	
	</Page>
</core:View>